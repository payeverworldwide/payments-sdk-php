{
  "name":"payever/payments-sdk-php",
  "type": "library",
  "description": "PHP SDK for payever payments",
  "keywords": ["payments","payever","pay","e-commerce"],
  "homepage": "https://github.com/payeverworldwide/payments-sdk-php",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Igor Siaryi",
      "email": "igor.siary@gmail.com",
      "role": "1.x and further development"
    }
  ],
  "require":{
    "php":">=5.4.0",
    "ext-json": "*",
    "ext-curl": "*",
    "payever/core-sdk-php": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "behat/behat": "^3.4",
    "laminas/laminas-diactoros": "^2.5",
    "laminas/laminas-httphandlerrunner": "^1.4",
    "squizlabs/php_codesniffer": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.18",
    "phpmd/phpmd": "^2.9"
  },
  "autoload":{
    "classmap":[
      "lib"
    ]
  },
  "autoload-dev":{
    "classmap":[
      "lib",
      "tests"
    ],
    "psr-4": {
      "Payever\\Tests\\": "tests/"
    }
  }
}
